<!DOCTYPE html>
<html>
<head>
	<meta charset="utf-8">
	<meta http-equiv="X-UA-Compatible" content="IE=edge">
	<title>Asistencia prueba estatica</title>
	<style>
		.container{
			display: flex;
			flex-direction: column;
			align-items: center;
		}
	</style>
	    <link rel="stylesheet" href="/css/styles.css"></script>
</head>
<body>
		<svg viewBox="0 0 960 300">
				<symbol id="s-text">
					<text text-anchor="middle" x="50%" y="80%">Escanea</text>
				</symbol>
			
				<g class = "g-ants">
					<use xlink:href="#s-text" class="text-copy"></use>
					<use xlink:href="#s-text" class="text-copy"></use>
					<use xlink:href="#s-text" class="text-copy"></use>
					<use xlink:href="#s-text" class="text-copy"></use>
					<use xlink:href="#s-text" class="text-copy"></use>
				</g>
			</svg>
	<div class="container">
		<!-- <h1>Por favor marque su asistencia</h1> -->
		<canvas id="codigo"></canvas>
	</div>
	
	
</body>
<script src="../node_modules/qrcode/build/qrcode.min.js"></script> 
<script>
var canvas = document.getElementById('codigo')
canvas.width  = 259;
canvas.height  = 259;
context = canvas.getContext('2d');

arreglito();
function arreglito()
{
  codigoqr = new Image();
  codigoqr.src = '<%= imagensita %>';
  codigoqr.onload = function(){
	context.drawImage(codigoqr, 0, 0);
  }
  logo = new Image();
  logo.src = 'data:image/png;base64,';
  logo.onload = function(){
	context.drawImage(logo, canvas.width / 2.5, canvas.height /2.5);
  }
}
</script>
</html>
